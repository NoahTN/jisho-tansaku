{
  "manifest_version": 3,
  "name": "Jisho Tansaku",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "host_permissions": [
    "https://jisho.org/*"
  ],
  "author": "NoahTN",
  "action": {
    "default_popup": "index.html",
    "default_title": "React Chrome Extension",
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "content_scripts": [{
    "js": [ "content.js" ],
    "matches": ["<all_urls>"]
  }],
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  }
}
